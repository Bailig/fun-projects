{
  "name": "fun-projects",
  "version": "0.0.0",
  "description": "Some web projects for fun",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "author": "Bailig",
  "homepage": "https://bailig.github.io/fun-projects",
  "license": "MIT",
  "scripts": {
    "start": "yarn clean && lerna run start --stream",
    "build": "yarn clean && lerna run build --stream",
    "clean": "lerna run clean --stream",
    "test": "echo \"Don't forget to add test!\"",
    "lint": "eslint --ext '.js,.jsx,.ts,.tsx' .",
    "format": "prettier --ignore-path '.gitignore' --write '**/*.{ts,tsx,js,jsx}'",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy": "yarn build && gh-pages -d packages/portal/dist -m '[skip ci] Updates'"
  },
  "engines": {
    "node": "^14.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@hot-loader/react-dom": "^16.13.0",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/react": "^6.0.21",
    "@types/jest": "^26.0.13",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.0.0",
    "babel-loader": "^8.1.0",
    "eslint": "^6.0.0",
    "eslint-config-airbnb-typescript": "^6.3.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.3.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-monorepo": "^0.2.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^4.1.0",
    "eslint-plugin-unicorn": "^15.0.1",
    "file-loader": "^6.1.0",
    "html-webpack-plugin": "^4.4.1",
    "jest": "^26.4.2",
    "lerna": "^3.22.1",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.1.1",
    "react-is": "^16.13.1",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "gh-pages": "^3.1.0"
  }
}
